{
  "name": "subtitle-merger",
  "version": "1.0.0",
  "description": "A library to merge two subtitles.",
  "keywords": [
    "srt",
    "subrip",
    "subtitle",
    "subtitle merger",
    "merge subtitle",
    "merge srt"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/umd/index.js",
  "module": "./dist/es/index.js",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/umd/index.js"
    }
  },
  "types": "dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^17.0.12",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  }
}
